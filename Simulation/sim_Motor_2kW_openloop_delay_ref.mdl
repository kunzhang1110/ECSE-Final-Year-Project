      ",fb_phase_b,ref_a_prev,Timer1)\n%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%\n%                            "
      "                                                                                                                "
      "                   %\n%           The aim is to:                                                                "
      "                                                                      %                                         "
      "                  \n%           generate 3-phase PWM waveform using principle of PWM in simulink                "
      "                                                                    %                      \n%           Version"
      " 1, written by Kun Zhang                                                                                        "
      "                             %                                          \n%                                     "
      "                                                                                                                "
      "          %\n%           The input parameters are                                                               "
      "                                                             %                                               \n%"
      "           phase_a: reference signal a                                                                          "
      "                                               %                              \n%           phase_b: reference s"
      "ignal b                                                                                                         "
      "                %         \n%           t: running time                                                         "
      "                                                                            %                      \n%          "
      " fb_phase_a: feedback signal a                                                                                  "
      "                                     %                                             \n%           fb_phase_a: fee"
      "dback signal b                                                                                                  "
      "                     %\n%                                                                                       "
      "                                                                        %\n%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
      "%%%%%%%%%%%%\n%space reserved for gloabal variables declaration\n\n\n%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
      "%%%%%%\n%Parameter settings\nREF_AMP= 1;                         %magnitude of reference\nREF_FREQ= 300;        "
      "              %frequency of reference\nAMP_MOD= 0.9;                       %amplitude modulation ratio=M(control"
      ")/M(tri)\nFREQ_MOD= 5;                      %frequency modulation ratio=f(tri)/f(control)\n\nTRI_AMP= REF_AMP/AM"
      "P_MOD;           %triangluar amplitude\nTRI_FREQ= FREQ_MOD*REF_FREQ;        %triangular frequency\nTRI_PER= 1/TR"
      "I_FREQ;                %triangular period\n\n%SAMP_FREQ= 10*1000000;              %sampling frequency\n%SAMP_PER"
      "= 1/SAMP_FREQ;              %sampling period\n\n\n%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%\nref_c= -ref"
      "_a-ref_b;                                                            %phase_c obatined by phase_a and phase_b\nt"
      "ri_wav= (2*TRI_AMP/(TRI_PER/2))*abs(mod(t,TRI_PER)-TRI_PER/2)-TRI_AMP;         %triangular wave \n\nif      ref_"
      "a>tri_wav;              \n        if      ref_a_prev<tri_wav                                              %if it"
      " is a falling edge for switch 2\n                pwm_1= 0;               pwm_2=0;                               "
      " %if yes, hold pwm_1   \n                Timer1= 1;                                                      %Timer1"
      " is 1 indicates the onset of delay\n        else    Timer1= 0;                                                  "
      "    \n        end\n        if      Timer1>0&&Timer1<2                                             %Time1<22*1e-0"
      "7=2.2us dalay period     \n                pwm_1= 0;               pwm_2=0;                                %if y"
      "es, hold pwm_1, increment Timer 1          \n                Timer1= Timer1+1; \n        else    pwm_1= REF_AMP;"
      "         pwm_2=0;                                %if Time==0(not an edge) or Timer>=21(beyond delay period)\n   "
      "             Timer1= 0;                                                      %set Timer to 0\n        end\nelse "
      "                                                                           %if ref_a<tri_wav; \n        if      "
      "ref_a_prev>tri_wav                                              %if it is a falling edge for switch 1\n         "
      "       pwm_1=0;                pwm_2= 0;                               %if yes, hold pwm_2  \n                Ti"
      "mer1= 1;\n        else    Timer1= 0;                                                      %Timer1 = 1 indicates "
      "the onset of delay\n        end\n        if      Timer1>0&&Timer1<100                                           "
      " %Time1<22*1e-07=2.2us dalay period     \n                pwm_1=0;          pwm_2= 0;                           "
      "          %if yes, hold pwm_1, increment Timer 1          \n                Timer1= Timer1+1; \n        else    "
      "pwm_1=0;                pwm_2= REF_AMP;                               \n        end           \nend\n\n\n\nif   "
      "   (ref_b>tri_wav);                pwm_3= REF_AMP;                 \nelse                                    pwm"
      "_3= 0;  \nend\n\nif      (ref_c>tri_wav);                pwm_5= REF_AMP; \nelse                                 "
      "   pwm_5= 0; \nend\n\n% Delay\n% pause on;                pause(2.1*10^-6);                pause off;           "
      "     %pausing for switching time (typically less than 1us)\n\n\n%upper switch and lower switch are pairwise inve"
      "reted\n% if      pwm_1==0;                       pwm_2= REF_AMP;\n% else                                    pwm_"
      "2= 0;\n% end\n\nif      pwm_3==0;                       pwm_4= REF_AMP;\nelse                                   "
      " pwm_4= 0;\nend\n\nif      pwm_5==0;                       pwm_6= REF_AMP;\nelse                                "
      "    pwm_6= 0;\nend\n\n% %Delay\n% pause on;                pause(2.1*10^-6);                pause off;          "
      "      %pausing for switching time (typically less than 1us)\n\n%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%"
      "\nctrl_sig= [pwm_1 pwm_2 pwm_3 pwm_4 pwm_5 pwm_6];        %PWM output\nmag_fb= 200;                             "
      "               %magnitude of feedback\nerror= fb_phase_a-mag_fb*ref_a;                         %error\nTimer1_ou"
      "t= Timer1;\nref_a_prev_out= ref_a;\n\n%plot inverter device voltage\n%plot(motor_2kW_Inverter.time,motor_2kW_Inv"
      "erter.signals.values)\n\n\n\n"
